# Example array
arr = [12, 5, 19, 7, 3, 25, 18]

# Initialize the largest and smallest with the first element of the array
largest = smallest = arr[0]

# Loop through the array to find the largest and smallest elements
for num in arr:
    if num > largest:
        largest = num
    if num < smallest:
        smallest = num

# Print the results
print("Largest element:", largest)
print("Smallest element:", smallest)
